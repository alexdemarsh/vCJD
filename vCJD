## Case 3 ---
Case3_ARE1 <- prob(0.4, 0.0001, incub)
Case3_USA <- prob(2.6, 0.4, incub)
Case3_BGD <- prob(6.6, 2.6, incub)
Case3_ARE2 <- prob(10.6, 6.6, incub)
Case3_SAU <- prob(23, 10.6, incub)

