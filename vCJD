incub <- read.table("/PATH/Baseline-results-subset.csv", header=T, sep = ",")
age <- read.table("/PATH/age-subset.csv", header=T, sep = ",")

# Function to compute probability and CI ----
prob <- function (a, b, data) {
    pr <- vector("numeric", nrow(data))
    for (i in 1:nrow(data)) {
        row <- data[i,]
        pr[[i]] <- pgamma(q = a, shape = row$shape, scale= row$scale) - pgamma(q = b, shape = row$shape, scale = row$scale)
    }
    pr <- sort(pr)
    CI <- vector ("numeric", 2)
    CI[1] <- pr[50]
    CI[2] <- pr[1958]
    print(median(pr))
    print(CI)
}

# Incubation period only, ungrouped ----
## Case 1 ---
Case1_SAU1 <- prob(4.5, 0.0001, incub)
Case1_USA <- prob(5.5, 4.5, incub)
Case1_SAU2 <- prob(32, 5.5, incub)

## Case 2 ---
Case2_USA <- prob(0.06, 0, incub)
Case2_EGY <- prob(7.2, 0.6, incub)
